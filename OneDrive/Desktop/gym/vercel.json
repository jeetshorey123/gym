{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/gym",
      "destination": "/"
    },
    {
      "source": "/gym/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/workout",
      "destination": "/dashboard"
    },
    {
      "source": "/training",
      "destination": "/dashboard"
    },
    {
      "source": "/exercise",
      "destination": "/dashboard"
    },
    {
      "source": "/progress",
      "destination": "/dashboard"
    },
    {
      "source": "/analytics",
      "destination": "/dashboard"
    },
    {
      "source": "/signin",
      "destination": "/login"
    },
    {
      "source": "/auth",
      "destination": "/login"
    },
    {
      "source": "/register",
      "destination": "/login"
    },
    {
      "source": "/user/:username",
      "destination": "/login?user=:username"
    },
    {
      "source": "/api/health",
      "destination": "/db-test"
    },
    {
      "source": "/health",
      "destination": "/db-test"
    },
    {
      "source": "/status",
      "destination": "/db-test"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}